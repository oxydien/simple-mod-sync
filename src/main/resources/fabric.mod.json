	"authors": [
		"oxydien"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/simple-mod-sync",
		"sources": "https://github.com/oxydien/simple-mod-sync"
	},
	"license": "MIT",
	"icon": "assets/simple-mod-sync/icon.png",
